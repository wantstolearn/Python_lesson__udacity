
def biggest(a,b,c):
    if a>=b and a>=c:
       return a
    if b>=a and b>=c:
        return b
    if c>=a and c>=b:
        return c
    
print biggest (3,3,6)


# nicer alternative

def bigger (a,b):
    if a > b :
        return a
    else :
        return b

def biggest(a,b,c):
    return bigger (bigger (a,b),c)

print biggest (6,9,6)
