import time

def time_execution(add) :
    start = time.clock()
    result = eval('add')
    run_time = time.clock()-start
    return result, run_time

def time_execution(times) :
    start = time.clock()
    result = eval('times')
    run_time = time.clock()-start
    return result, run_time

def add (a,b):
        out=a+b
        return out

def times (a,b):
        out=a*b*a*b
        return out
  
print time_execution (add(4,5))
print time_execution (times(10000000,100000000))
                
